.weui-grid {
    position: relative;
    float: left;
    padding: 20px 10px;
    width: 33.33333333%;
    box-sizing: border-box;
    border-right: 1rpx solid #D9D9D9;
    border-bottom: 1rpx solid #D9D9D9;
}
.weui-grid__icon {
    display: block;
    width: 68px;
    height: 68px;
    margin: 0 auto;
	border-radius: 50%;
}
